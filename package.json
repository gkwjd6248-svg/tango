{
  "name": "tango-community",
  "version": "1.0.0",
  "private": true,
  "workspaces": ["backend", "mobile", "crawler", "web"],
  "scripts": {
    "backend:dev": "npm run dev --workspace=backend",
    "mobile:dev": "npm run dev --workspace=mobile",
    "crawler:dev": "npm run dev --workspace=crawler",
    "crawler:run": "npm run crawl --workspace=crawler",
    "crawler:local": "npm run local --workspace=crawler",
